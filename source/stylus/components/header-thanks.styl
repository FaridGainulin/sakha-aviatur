.header-thanks
  &__phone-number-wrapper
    color: #fff
    border 2px solid #fff
    border-radius: 8px
    display: flex
    justify-content center
    align-items: center
    width 110px
    height: 25px
    +above($breakpoints.sm)
      border-radius: 13px
      width 140px
      height: 30px
    +above($breakpoints.md)
      width 160px
      height: 35px
    +above($breakpoints.xxl)
      width 175px
      height: 40px

